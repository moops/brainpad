<% if (@journals and @journals.size > 0) %>
<table width="100% cellpadding="2" cellspacing="0" class="searchResult">
    <tr>
        <th>date</th>
        <th>type</th>
        <th>entry</th>
        <th colspan="3">&nbsp;</th></tr>

        
<%= render(:partial => 'data_rows', :collection => @journals) %>
</table>
<% end %>

<%= will_paginate @journals %>

<h3 class="formHeader"><%= @form_header %></h3>
<div id="form" class="form">
	<%= render(:partial => 'form') %>
</div>