<% if (@workouts and @workouts.size > 0) %>
<table width="100% cellpadding="2" cellspacing="0" class="searchResult">
    <tr>
        <th>date</th>
        <th>type</th>
        <th>location</th>
        <th>distance</th>
        <th>duration</th>
        <th colspan="3">&nbsp;</th></tr>
    
    <%= render(:partial => 'data_rows', :collection => @workouts) %>
</table>
<% end %>

<%= will_paginate @workouts %>

<p>
